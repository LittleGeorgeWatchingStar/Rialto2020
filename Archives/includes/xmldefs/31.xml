<vendor_definition vendor_name='Avnet'>
	<start_word value="CUST"></start_word>
	<end_word value="RETURN"></end_word>

	<table_definition name='tables'>
                <field name='custline' order='0'></field>
		<field name='line' order='1'></field>
                <field name='shipped' order='2'></field>
		<field name='description' order='3~-3'></field>
		<field name='mfg' order='-3'></field>
		<field name='each' order='-2'></field>
		<field name='total' order='-1'></field>
	</table_definition>

	<globals_definition name='globals'>
		<global name='purchase_order_no' text='CUSTOMER P.O. NUMBER' location='below_1'></global>
		<global name='invoice_no' text='INVOICE NO.' location='below_1' right='eol'></global>
		<global name='sales_order_no' text='SALES CONTROL NO.' right='eol' location='below_1'></global>
		<global name='invoice_date' text='INVOICE DATE' location='below_1'></global>
		<global name='terms' ></global>
		<global name='tracking_number' text='WAYBILL' location='below_1' right='PACKING'></global>
		<global name='freight_charges' text='HANDLING CHARGES' location='right'></global>
		<global name='tax_charged' ></global>
		<global name='grand_total' text='INVOICE TOTAL' location='below_1' right='eol' ></global>
	</globals_definition>

</vendor_definition>
